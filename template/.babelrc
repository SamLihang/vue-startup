{
  "presets": [
    [
      "env", {
        "targets": {
          "browsers": ["last 2 versions", "safari > 7"]
        },
        "modules": false,
        "useBuiltIns": "entry",
        "loose": true
      }
    ],
    "stage-2"
  ],
  {{#if_eq state 'mobx'}}
  "plugins": ["transform-decorators-legacy", "transform-class-properties"]
  {{/if_eq}}
}
