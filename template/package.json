{
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "author": "{{author}}",
  "license": "MIT",
  "scripts": {
    "prepush": "npm run ilint -q",
    "dll": "npx webpack --config webpack.dll.config.js",
    "dev": "npx cross-env NODE_ENV=development node ./build/dev-server.js",
    "build": "npx cross-env NODE_ENV=production npx webpack --config ./build/webpack.prod.config.js --hide-modules",
    "ilint": "npx eslint src/**/*.js src/**/*.vue",
    "fix": "npx eslint --fix src/**/*.js src/**/*.vue",
    "commitmsg": "npx commitlint -e"
  },
  "dependencies": {
    "vue": "^2.5.8",
    "vue-router": "^3.0.1",
    {{#if_eq state 'vuex'}}
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
    {{/if_eq}}
    {{#if_eq state 'revue'}}
    "revuejs": "^0.7.7",
    {{/if_eq}}
    {{#if_eq state 'mobx'}}
    "mobx": "^4.0.1",
    "vue-mobx": "^2.0.0",
    {{/if_eq}}
    "async-await-error-handling": "^0.5.2",
    "axios": "^0.16.2",
    "normalize.css": "^7.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^5.2.8",
    "@commitlint/config-angular": "^5.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.1.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "eslint": "^4.8.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-vue": "^2.1.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "gulp-util": "^3.0.8",
    "happypack": "^4.0.0",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "open-browser-webpack-plugin": "^0.0.5",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.6",
    "husky": "^0.14.3",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.5.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.8",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-parallel-uglify-plugin": "^1.0.0",
    "progress-bar-webpack-plugin": "^1.10.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 5.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
